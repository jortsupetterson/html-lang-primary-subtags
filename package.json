{
  "name": "html-lang-primary-subtags",
  "version": "1.0.3",
  "description": "TypeScript union type for primary language subtags (ISO-639/IANA) usable in HTML lang attribute.",
  "keywords": [
    "typescript",
    "types",
    "html",
    "lang",
    "bcp47",
    "iso-639",
    "language",
    "subtag",
    "union"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jortsupetterson/html-lang-primary-subtags.git"
  },
  "bugs": {
    "url": "https://github.com/jortsupetterson/html-lang-primary-subtags/issues"
  },
  "homepage": "https://github.com/jortsupetterson/html-lang-primary-subtags#readme",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  }
}
